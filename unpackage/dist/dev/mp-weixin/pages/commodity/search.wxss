.cate-mask {
  position: fixed;
  left: 0;
  top: 100rpx;
  bottom: 50px;
  width: 100%;
  background: transparent;
  z-index: 95;
  -webkit-transition: .3s;
  transition: .3s;
  height: calc(100% - 64px);
}
.cate-mask .cate-content {
    width: 66.7%;
    height: calc(100% - 100rpx - env(safe-area-inset-bottom) / 2);
    background: #fff;
    float: right;
    -webkit-transform: translateX(100%);
            transform: translateX(100%);
    -webkit-transition: .3s;
    transition: .3s;
    position: relative;
}
.cate-mask.none {
    display: none;
}
.cate-mask.show {
    background: rgba(0, 0, 0, 0.4);
}
.cate-mask.show .cate-content {
      -webkit-transform: translateX(0);
              transform: translateX(0);
      height: 100%;
}
