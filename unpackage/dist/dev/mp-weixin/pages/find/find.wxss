
page{
		background-color: #f9f9f9 ;
}
.find_box{
		width: 100%;
}
.find_list{
		width: 100%;
		height: auto;
		margin-bottom: 40rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		
		-webkit-box-orient: vertical;
		
		-webkit-box-direction: normal;
		
		-webkit-flex-direction: column;
		
		        flex-direction: column;
		background-color: #fff;
}
.img_box{
		position: relative;
		padding-bottom: 56.3%;
		width: 100%;
		height: 0;
}
.img_box image{
		position: absolute;
		left: 0;
		top: 0;
		height: 100%;
	    width: 100%;
}
.find_list .title{
		font-size: 36rpx;
		font-weight: bold;
		margin-top: 18px;
		margin-bottom: 5px;
		padding: 0px 32px;
}
.find_list .sub_title{
		font-size: 32rpx;
		overflow: hidden;
		text-overflow: ellipsis;
		display: -webkit-box;
		-webkit-line-clamp: 2;
		-webkit-box-orient: vertical;
		margin-bottom: 25px;
		text-indent: 64rpx;
		padding: 0 32px;
		line-height: 28px;
		color: #666;
}
.details{
		width: 100%;
		height: 50px;
		padding: 0 18px;
		font-size: 32rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		-webkit-box-pack: end;
		-webkit-justify-content: flex-end;
		        justify-content: flex-end;
		border-top: 0.2rpx solid #e1e1e1;
		border-bottom: 0.2rpx solid #e1e1e1;
		color: #666;
}

